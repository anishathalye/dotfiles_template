# po
Host po
    HostName 127.0.0.1
    User vagrant
    Port 2222
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    PasswordAuthentication no
    IdentityFile $CHEF_DIR/.vagrant/machines/po/virtualbox/private_key
    IdentitiesOnly yes
    LogLevel FATAL
    ForwardAgent yes

