# MAIN OPTIONS
VisualHostKey=no


Host nec* nsp* aws* po*
    PermitLocalCommand yes
    LocalCommand tar ch -C${HOME} .gitconfig .screenrc .initsys.sh | ssh -o PermitLocalCommand=no %n "tar mx -C${HOME}"

Host github.com
    IdentityFile ~/.ssh/id_rsa
    Hostname ssh.github.com

# UTAS GATEWAY
Host its6
    Hostname 6-its-hob.emii.org.au
    User lbesnard
    Port 22
    ForwardAgent yes

# Nectar machines hobart
Host nec4
    Hostname 4-nec-hob.emii.org.au
    User lbesnard
    Port 22
    ForwardAgent yes

Host nec14
    Hostname 14-nec-hob.emii.org.au
    User lbesnard
    Port 22
    ForwardAgent yes

# sydney
Host aws10
    Hostname 10-aws-syd.emii.org.au
    User lbesnard
    Port 22
    ForwardAgent yes

# po
Host po
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/lbesnard/github_repo/chef/.vagrant/machines/po/virtualbox/private_key
  IdentitiesOnly yes
  LogLevel FATAL
  ForwardAgent yes

# Nectar
Host nec10
    Hostname 10-nec-hob.emii.org.au 
    User lbesnard
    Port 22
    ForwardAgent yes

Host nec2
    Hostname 2-nec-hob.emii.org.au
    User lbesnard
    Port 22
    ForwardAgent yes

# katabatic ACECRC
Host katabatic
     hostname acecompute.antcrc.utas.edu.au
     user lbesnard
     port 22

# gcw0
Host gcw0_wlan
    hostname 10.1.2.1
    user root
    port 22
    IdentityFile ~/.ssh/id_dsa.pub

Host gcw0_usb0
    hostname 10.1.1.2
    user root
    port 22
    IdentityFile ~/.ssh/id_rsa.pub

# tunnel through its6
Host nec10_tunnel
    Hostname  10-nec-hob.emii.org.au
    Proxycommand ssh -q its6 nc -q0 %h  22
    ForwardAgent yes

Host nec14_tunnel
    Hostname 14-nec-hob.emii.org.au
    Proxycommand ssh -q its6 nc -q0 %h  22
    ForwardAgent yes

